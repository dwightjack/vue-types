{
  "name": "vue-types-monorepo",
  "description": "VueTypes Monorepo",
  "author": "Marco Solazzi",
  "license": "MIT",
  "homepage": "https://dwightjack.github.io/vue-types/",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=16.16.0"
  },
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "examples:dev": "npm run build --workspace=vue-types && npm start --workspace=examples",
    "docs:dev": "npm run dev --workspace=docs",
    "lint": "npm run lint --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "release:next": "npm run build && npx changeset publish --tag=next",
    "release:stable": "npm run build && npx changeset publish"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dwightjack/vue-types.git"
  },
  "bugs": "https://github.com/dwightjack/vue-types/issues",
  "devDependencies": {
    "@changesets/cli": "2.25.2",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "@vitest/coverage-c8": "0.25.3",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "happy-dom": "7.6.7",
    "lefthook": "1.2.3",
    "prettier": "2.6.2",
    "typescript": "4.6.4",
    "vite": "3.2.4",
    "vitest": "0.25.3"
  }
}
