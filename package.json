{
  "name": "vue-types-monorepo",
  "description": "VueTypes Monorepo",
  "author": "Marco Solazzi",
  "license": "MIT",
  "homepage": "https://dwightjack.github.io/vue-types/",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=16.16.0"
  },
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "examples:dev": "npm run build --workspace=vue-types && npm start --workspace=examples",
    "docs:dev": "npm run dev --workspace=docs",
    "lint": "npm run lint --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "release": "npm run build && npx changeset publish"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dwightjack/vue-types.git"
  },
  "bugs": "https://github.com/dwightjack/vue-types/issues",
  "devDependencies": {
    "@changesets/cli": "2.26.0",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "@vitest/coverage-c8": "0.29.2",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "happy-dom": "8.9.0",
    "lefthook": "1.3.3",
    "prettier": "2.8.4",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.2"
  }
}
